{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/pipzass-front/browser" }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/main-(.*)\\.js", "dest": "/main-$1.js" },
    { "src": "/polyfills-(.*)\\.js", "dest": "/polyfills-$1.js" },
    { "src": "/scripts-(.*)\\.js", "dest": "/scripts-$1.js" },
    { "src": "/styles-(.*)\\.css", "dest": "/styles-$1.css" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


